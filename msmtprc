account personal
host smtp.gmail.com
user christofferdall@christofferdall.dk
password secret
auth
port 587
maildomain christofferdall.dk
auto_from off
from cdall@cs.columbia.edu
logfile /var/log/msmtp/msmtp.log
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account linaro
host smtp.gmail.com
